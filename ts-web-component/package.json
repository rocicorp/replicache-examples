{
  "name": "replicache-todo-ts-component",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "dev": "vite",
    "build": "npm run lint && tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write './**/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'",
    "check-format": "prettier --check './**/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "replicache": ">=11.2.1",
    "replicache-react": "^2.9.0"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.1.1",
    "prettier": "^2.2.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": {
    "singleQuote": false,
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": true,
    "tabWidth": 2,
    "useTabs": false
  }
}
