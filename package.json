{
  "name": "replicache-todo-react",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "dev": "vite",
    "dev-worker": "wrangler dev",
    "build": "npm run lint && tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write './src/*/*.{js,jsx,json,ts,tsx,html,css,md}' '*.{cjs,js,jsx,json,ts,tsx,html,css,md}'"
  },
  "devDependencies": {
    "@rocicorp/eslint-config": "^0.1.0",
    "@rocicorp/rails": "^0.2.2",
    "@rocicorp/reflect": "^0.10.0",
    "@rocicorp/reflect-server": "^0.19.0-beta.9",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "classnames": "^2.3.1",
    "nanoid": "^4.0.0",
    "prettier": "^2.2.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "replicache-react": "^2.10.0",
    "todomvc-app-css": "^2.4.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.7",
    "zod": "^3.19.1"
  },
  "eslintConfig": {
    "extends": "@rocicorp/eslint-config"
  },
  "prettier": {
    "singleQuote": false,
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": true,
    "tabWidth": 2,
    "useTabs": false
  }
}
